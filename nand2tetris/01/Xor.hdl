/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Nand(a=a, b=a, out=notA);
    Nand(a=b, b=b, out=notB);
    Nand(a=notA, b=b, out=f2);
    Nand(a=a, b=notB, out=f3);
    Nand(a=f2, b=f3, out=out);
}